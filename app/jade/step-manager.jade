.app-launch.lexi
  .step-manager
    .step-title-bar
      .step-count Step <span id='current-step'>1</span> of <span id='total-steps'>2 : </span>
      .step-title Name your App
      .ui-btn.back
        img.shadow-icon(data-src="back-x-btn")
        .txt Back
      .ui-btn.cancel 
        img.shadow-icon(data-src="cancel-x-btn")
        .txt Cancel
    .step-wrapper
      .steps
